{
    "include": ["./**/*.ts", "../cypress.d.ts"],
    "compilerOptions": {
    "target": "es5",
    "types": ["cypress", "node"],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react",
    "moduleResolution": "node"
  },
  }